                      <whlabel1 value="Font (active):" hflex="1" />
                      <cell valign="middle" sclass="font_preview"><label id="whdSearchTabFontActivePrev" sclass="font_prev_inner" value="Abc" /></cell>
                      <button id="whdSearchTabFontActiveBtn" label="..." />
                    </hbox>
                    <hbox align="center" width="100%" spacing="3px">
                      <whlabel1 value="Font (inactive):" hflex="1" />
                      <cell valign="middle" sclass="font_preview"><label id="whdSearchTabFontPrev" sclass="font_prev_inner" value="Abc" /></cell>
                      <button id="whdSearchTabFontBtn" label="..." />
                    </hbox>
                    <hbox align="center" width="100%" spacing="3px">
                      <whlabel1 value="Font (mouse-over):" hflex="1" />
                      <cell valign="middle" sclass="font_preview"><label id="whdSearchTabFontHoverPrev" sclass="font_prev_inner" value="Abc" /></cell>
                      <button id="whdSearchTabFontHoverBtn" label="..." />
                    </hbox>
                    <whlabel1 value="Panel Padding:" />
                    <hbox pack="stretch" align="center" width="100%" spacing="0px">
                      <cell width="50%" align="right">
                        <whlabel2 value="Top&#160;[px]:" />
                        <spinner id="whdSearchPanelPaddingTopBox" constraint="no negative" cols="4" hflex="min" />
                      </cell>
                      <cell width="50%" align="right">
                        <whlabel2 value="Bottom&#160;[px]:" />
                        <spinner id="whdSearchPanelPaddingBottomBox" constraint="no negative" cols="4" hflex="min" />
                      </cell>
                    </hbox>
                    <hbox pack="stretch" align="center" width="100%" spacing="0px">
                      <cell width="50%" align="right">
                        <whlabel2 value="Left&#160;[px]:" />
                        <spinner id="whdSearchPanelPaddingLeftBox" constraint="no negative" cols="4" hflex="min" />
                      </cell>
                      <cell width="50%" align="right">
                        <whlabel2 value="Right&#160;[px]:" />
                        <spinner id="whdSearchPanelPaddingRightBox" constraint="no negative" cols="4" hflex="min" />
                      </cell>
                    </hbox>
                    <hbox align="center" width="100%" spacing="3px">
                      <whlabel1 value="Panel Background:" hflex="1" />
                      <div sclass="box_preview"><div id="whdSearchPanelBgPrev" sclass="box_prev_inner"></div></div>
                      <button id="whdSearchPanelBgBtn" label="..." />
                    </hbox>
                    <!-- </div> -->
                  </vbox>
                </tabpanel>
              </tabpanels>
            </tabbox>
        </vbox>
      </west>
      <center border="normal">
        <!-- <splitter collapse="before" /> -->
        <borderlayout hflex="1" height="100%" style="background-color:#070604;">
          <north id="whdPreviewControls" height="50px" margins="0,10,10,0" style="background-color:#070604;" border="none">
            <hbox pack="stretch" align="center" hflex="1" height="50px" >
              <!-- <hbox id="whdButtonsArea" align="center" width="100%" spacing="3px"> -->
              <button label="Refresh Preview" id="whdPreviewRefreshBtn" image="img/refresh_icon.gif" orient="horizontal"
                      style="min-width:180px; min-height:26px;" />
              <space spacing="12px" />
              <button label="Save" id="whdSaveBtn" image="img/save.gif" orient="horizontal"
                      style="white-space:nowrap; min-width:90px; min-height:26px;" />
              <button label="Save as..." id="whdSaveAsBtn" image="img/save_as.gif" orient="horizontal"
                      style="white-space:nowrap; min-width:90px; min-height:26px;" />
              <button label="Delete" id="whdDeleteBtn" image="img/delete.gif" orient="horizontal"
                      style="white-space:nowrap; min-width:90px; min-height:26px;" />
              <!-- </hbox> -->
              <space spacing="16px" />
              <head1label value="Design:" />
              <head2label id="whdDesignNameLabel" value="" />
              <space spacing="16px" />
              <head1label value="Preview:" />
              <space spacing="4px" />
              <checkbox id="whdPreviewPartsCheckbox" />
              <head2label value="Tabs" />
              <space spacing="4px" />
              <checkbox id="whdPreviewCrumbsCheckbox" />
              <head2label value="Breadcrumbs" />
              <timer id="whdPreviewTimer" repeats="false" delay="500" running="false" />
              <space hflex="1" />
              <headtextbutton label="Logout" id="whdExitBtn" image="img/on_off.gif" 
                              style="font-weight:bold; white-space:nowrap;"/>
              <space spacing="10px" />
            </hbox>
          </north>
          <center border="normal">
            <iframe id="whdPreviewFrame" name="whdPreviewFrame" src="preview_empty.html" width="100%" height="100%"/>
          </center>
        </borderlayout>
      </center>
    </borderlayout>

    <window id="whdLoginWin" title="WebHelp Designer ${c:l('label.login.windowtitle')}" border="normal"
            mode="overlapped" position="center" >
        <caption>
            <!-- <hbox spacing="8px"> -->
                <toolbarbutton label="${c:l('label.help')}" id="whdLoginHelpBtn" />
            <!-- </hbox> -->
        </caption>

        <hbox spacing="4px">
            <image src="img/login_pic.jpg" height="118px" />
            <space spacing="3px" />
            <vbox spacing="3px" >
                <separator spacing="0px"/>
                <label>${c:l('label.login.username')}:</label>
                <textbox id="username" width="160px" forward="onOK=loginBtn.onClick"/>
                <separator spacing="1px"/>
                <label>${c:l('label.login.password')}:</label>
                <textbox id="userpwd" type="password"  width="160px"  forward="onOK=loginBtn.onClick"/>
                <separator spacing="2px"/>
                <button id="loginBtn" label="${c:l('label.login.btn')}" width="160px" />
                <!-- forward="onClick=whdLoginWin.onLoginClick" -->
            </vbox>
            <space spacing="3px" />
        </hbox>
    </window>

