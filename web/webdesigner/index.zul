<?xml version="1.0" encoding="UTF-8"?>
<?page title="Web-Designer" ?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>

<?component name="textbutton" extends="toolbarbutton" style="font-size:11px; color:blue;" ?>
<?component name="headtextbutton" extends="toolbarbutton" sclass="headBtnCls" ?>
<?component name="head1label" extends="label" style="color:#F0F0F0; font-weight:bold; white-space:nowrap;" ?>
<?component name="head2label" extends="label" style="color:#F0F0F0; font-weight:normal; white-space:nowrap;" ?>
<?component name="whlabel1" extends="label" style="font-size:12px; color:#000000; font-weight:bold; white-space:nowrap;" ?>
<?component name="whlabel2" extends="label" style="font-size:12px; color:#000000; font-weight:normal;" ?>

<zk>

<style>
.headBtnCls .z-toolbarbutton-cnt { color:#DFEEFF; font-weight:normal; font-size:11px; text-shadow:1px 1px 1px #202020; }
.z-toolbarbutton-over .z-toolbarbutton-cnt { color:#202020; text-shadow:0 0 0; }

.control_panel { background-color:#F0F0F0; padding:8px; }    
.box_preview { width:80px; height:auto; overflow:hidden; }
.box_preview_small { width:55px; height:auto; overflow:hidden; }
.box_prev_inner { width:50px; height:20px; float:right; }
.font_preview { width:55px; height:20px; overflow:hidden; }
.font_prev_inner { width:50px; padding:4px 0 4px 0; float:right; text-align:center; }
.img_selected { border:dashed 1px #D0D0D0; }
</style>

  <window id="whdMainWin"  height="100%" width="100%" border="none" mode="embedded"
          contentStyle="background-color:#303030;"
          apply="org.docma.webdesigner.WebDesignerComposer" >

    <borderlayout id="whdMainLayout" hflex="1" height="100%" style="background-color:#070604;">
      <!-- <north height="60px" margins="0,0,0,0" style="background-color:#070604;" border="none">
      </north> -->
      <west style="background-color:#181410; background-image:url(img/whd_back.jpg); background-repeat:no-repeat; background-size:auto 64px;" 
            border="none" margins="0,0,0,0" width="292px" splittable="true">
        <vbox hflex="1" align="stretch" style="padding:12px 8px 0 8px">
            <hbox pack="start" style="padding:0 0 0 0;">
                <image src="img/whd_logo.png" width="274px" />
            </hbox>
            <separator spacing="3px" />
            <hbox align="center" pack="stretch" width="100%" spacing="3px">
              <head1label value="Saved Designs:" hflex="1"/>
              <headtextbutton label="New" id="whdNewBtn" hflex="min" />
              <headtextbutton label="Import" id="whdImportBtn" hflex="min" />
              <headtextbutton label="Export" id="whdExportBtn" hflex="min" />
            </hbox>
            <hbox align="center" width="100%" spacing="3px">
              <listbox id="whdSelectList" mold="select" rows="1" hflex="1"></listbox>
            </hbox>
            <separator spacing="5px" />
            
            <tabbox id="whdControlsTabBox" width="98%" mold="accordion" panelSpacing="2px">
              <tabs>
                <tab label="General" />
                <tab label="Heading" />
                <tab label="Logo Images" />
                <tab label="Tree Navigation" />
                <tab label="Tree Style" />
                <tab label="Tab Navigation" />
                <tab label="Breadcrumbs Navigation" />
                <tab label="Previous/Next Navigation" />
                <tab label="Search" />
                <tab label="Search Result" />
                <tab label="Content/Search Tab" id="whdSearchTabArea" />
              </tabs>
              <tabpanels>
                <!-- General Panel -->
                <tabpanel sclass="control_panel">
                  <vbox align="stretch" width="100%" spacing="3px">
                    <hbox id="whdLayoutVersionArea" align="center" width="100%" spacing="4px">
                      <whlabel1 value="Layout:" hflex="1" />
                      <listbox id="whdLayoutIdBox" mold="select" rows="1"></listbox>
                      <whlabel2 value="Version" hflex="min" />
                      <listbox id="whdLayoutVersionBox" mold="select" rows="1"></listbox>
                    </hbox>
                    <hbox align="center" width="100%" spacing="3px">
                      <whlabel1 value="Favorite Icon:" hflex="1" />
                      <textbutton id="whdFaviconDownloadBtn" label="Download" />
                      <textbutton id="whdFaviconReplaceBtn" label="Replace" upload="true" />
                      <image id="whdFaviconImage" style="max-height:32px;max-width:32px;" />
                    </hbox>
                    <hbox align="center" width="100%" spacing="3px">
                      <whlabel1 value="Background Color:" hflex="1" />
                      <colorbox id="whdGeneralBgColorBox" />
                    </hbox>
                    <hbox align="center" width="100%" spacing="3px">
                      <whlabel1 value="Content Top Offset:" hflex="1" />
                      <spinner id="whdContentTopBox" constraint="no negative" value="0" cols="4" hflex="min" />
                      <label value="px" hflex="min" />
                    </hbox>
                    <whlabel1 value="Content Padding:" />
                    <hbox align="center" width="100%" spacing="4px">
                      <cell hflex="1" align="right">
                        <whlabel2 value="Left&#160;[px]:" />
                        <spinner id="whdContentPaddingLeftBox" constraint="no negative" width="60px" />
                      </cell>
                      <cell width="50%" align="right">
                        <whlabel2 value="Right&#160;[px]:" />
                        <spinner id="whdContentPaddingRightBox" constraint="no negative" width="60px" />
                      </cell>
                    </hbox>
                    <whlabel1 value="Content Width:" />
                    <hbox align="center" width="100%" spacing="4px">
                      <cell hflex="1" align="right">
                        <whlabel2 value="Min&#160;[px]:" />
                        <spinner id="whdContentWidthMinBox" constraint="no negative" width="60px" />
                      </cell>
                      <cell width="50%" align="right">
                        <whlabel2 value="Max&#160;[px]:" />
                        <spinner id="whdContentWidthMaxBox" constraint="no negative" width="60px" />
                      </cell>
                    </hbox>
                    <whlabel1 value="Watermark Position:" />
                    <hbox align="center" width="100%" spacing="4px">
                      <cell hflex="1" align="right">
                        <whlabel2 value="X-Pos&#160;[px]:" />
                        <spinner id="whdWatermarkXPosBox" constraint="no negative" width="60px" />
                      </cell>
                      <cell width="50%" align="right">
                        <whlabel2 value="Y-Pos&#160;[px]:" />
                        <spinner id="whdWatermarkYPosBox" constraint="no negative" width="60px" />
                      </cell>
                    </hbox>
                  </vbox>
                </tabpanel>
                <!-- Heading Panel -->
                <tabpanel sclass="control_panel">
                  <vbox align="stretch" width="100%" spacing="2px">
                    <hbox align="center" width="100%" spacing="3px">
                      <whlabel1 value="Header Height:" hflex="1" />
                      <spinner id="whdHeaderHeightBox" constraint="no negative" value="80" cols="4" hflex="min" />
                      <label value="px" hflex="min" />
                    </hbox>
                    <hbox align="center" width="100%" spacing="3px">
                      <whlabel1 value="Background/Border:" hflex="1" />
                      <div sclass="box_preview"><div id="whdHeaderBgPrev" sclass="box_prev_inner"></div></div>
                      <button id="whdHeaderBgBtn" label="..." />
                    </hbox>
                    <whlabel1 value="Padding:" />
                    <hbox pack="stretch" align="center" width="100%" spacing="0px">
                      <cell hflex="1" align="right">
                        <whlabel2 value="Top [px]:" />
                        <spinner id="whdHeaderPaddingTopBox" constraint="no negative" width="60px" />
                      </cell>
                      <cell width="50%" align="right">
                        <whlabel2 value="Bottom [px]:" />
                        <spinner id="whdHeaderPaddingBottomBox" constraint="no negative" width="60px" />
                      </cell>
                    </hbox>
                    <hbox pack="stretch" align="center" width="100%" spacing="0px">
                      <cell hflex="1" align="right">
                        <whlabel2 value="Left [px]:" />
                        <spinner id="whdHeaderPaddingLeftBox" constraint="no negative" width="60px" />
                      </cell>
                      <cell width="50%" align="right">
                        <whlabel2 value="Right [px]:" />
                        <spinner id="whdHeaderPaddingRightBox" constraint="no negative" width="60px" />
                      </cell>
                    </hbox>
                    <hbox align="center" width="100%" spacing="3px">
                      <checkbox id="whdHeaderTitle1Checkbox" />
                      <whlabel1 value="Title 1:" hflex="1" />
                      <cell valign="middle" sclass="font_preview"><label id="whdHeaderTitle1FontPrev" sclass="font_prev_inner" value="Abc" /></cell>
                      <button id="whdHeaderTitle1FontBtn" label="..." />
                    </hbox>
                    <hbox align="center" width="100%" spacing="3px">
                      <checkbox id="whdHeaderTitle2Checkbox" />
                      <whlabel1 value="Title 2:" hflex="1" />
                      <cell valign="middle" sclass="font_preview"><label id="whdHeaderTitle2FontPrev" sclass="font_prev_inner" value="Abc" /></cell>
                      <button id="whdHeaderTitle2FontBtn" label="..." />
                    </hbox>
                    <hbox align="center" width="100%" spacing="3px">
                      <whlabel1 value="Title Alignment:" hflex="1" />
                      <listbox id="whdHeaderTitleAlignBox" mold="select" rows="1" >
                        <listitem label="center" value="center"/>
                        <listitem label="left" value="left"/>
                        <listitem label="right" value="right"/>
                      </listbox>
                    </hbox>
                    <whlabel1 value="Title Margins:" />
                    <hbox pack="stretch" align="center" width="100%" spacing="0px">
                      <cell hflex="1" align="right">
                        <whlabel2 value="Top [px]:" />
                        <spinner id="whdHeaderTitleMarginTopBox" constraint="no negative" width="60px" />
                      </cell>
                      <cell width="50%" align="right">
                        <whlabel2 value="Bottom [px]:" />
                        <spinner id="whdHeaderTitleMarginBottomBox" constraint="no negative" width="60px" />
                      </cell>
                    </hbox>
                    <hbox pack="stretch" align="center" width="100%" spacing="0px">
                      <cell hflex="1" align="right">
                        <whlabel2 value="Left [px]:" />
                        <spinner id="whdHeaderTitleMarginLeftBox" constraint="no negative" width="60px" />
                      </cell>
                      <cell width="50%" align="right">
                        <whlabel2 value="Right [px]:" />
                        <spinner id="whdHeaderTitleMarginRightBox" constraint="no negative" width="60px" />
                      </cell>
                    </hbox>
                  </vbox>
                </tabpanel>
                <!-- Logo Images -->
                <tabpanel sclass="control_panel">
                  <hbox align="center" width="100%" spacing="3px">
                    <checkbox id="whdLogo1Checkbox" />
                    <whlabel1 value="Logo 1:" />
                    <textbutton id="whdLogo1ReplaceBtn" label="Upload" upload="true" />
                    <textbutton id="whdLogo1DownloadBtn" label="Download" />
                    <textbutton id="whdLogo1DeleteBtn" label="Delete" />
                  </hbox>
                  <div id="whdLogo1Area" width="100%">
                      <hbox align="center" width="100%" spacing="3px">
                        <space spacing="16px" />
                        <image id="whdLogo1Image" style="max-height:80px; max-width:200px;" />
                      </hbox>
                      <separator spacing="3px" />
                      <hbox align="center" width="100%" spacing="3px">
                        <space spacing="16px" />
                        <whlabel2 value="Position:" hflex="min" />
                        <listbox id="whdLogo1PositionBox" mold="select" rows="1" >
                          <listitem label="Top-left" value="left"/>
                          <listitem label="Top-left (absolute)" value="absolute-top-left"/>
                          <listitem label="Top-left (fixed)" value="fixed-top-left"/>
                          <listitem label="Top-right (absolute)" value="absolute-top-right"/>
                          <listitem label="Top-right (fixed)" value="fixed-top-right"/>
                          <listitem label="Bottom-left (absolute)" value="absolute-bottom-left"/>
                          <listitem label="Bottom-left (fixed)" value="fixed-bottom-left"/>
                          <listitem label="Bottom-right (absolute)" value="absolute-bottom-right"/>
                          <listitem label="Bottom-right (fixed)" value="fixed-bottom-right"/>
                        </listbox>
                      </hbox>
                      <separator spacing="3px" />
                      <hbox align="center" pack="stretch" width="100%" spacing="4px">
                        <space spacing="16px" />
                        <cell hflex="1">
                          <hbox align="center" pack="stretch">
                            <whlabel2 value="Width&#160;[px]:" />
                            <spinner id="whdLogo1WidthBox" constraint="no negative" value="1" cols="4" hflex="1" />
                          </hbox>
                        </cell>
                        <cell hflex="1">
                          <hbox align="center" pack="stretch">
                            <whlabel2 value="Height&#160;[px]:" />
                            <spinner id="whdLogo1HeightBox" constraint="no negative" value="1" cols="4" hflex="1" />
                          </hbox>
                        </cell>
                      </hbox>
                      <separator spacing="3px" />
                      <hbox align="center" pack="stretch" width="100%" spacing="4px">
                        <space spacing="16px" />
                        <cell hflex="1">
                          <hbox align="center" pack="stretch">
                            <whlabel2 value="X-Pos&#160;[px]:" style="white-space:nowrap;" />
                            <spinner id="whdLogo1XPosBox" constraint="" value="1" cols="4" hflex="1" />
                          </hbox>
                        </cell>
                        <cell hflex="1">
                          <hbox align="center" pack="stretch">
                            <whlabel2 value="Y-Pos&#160;[px]:" style="white-space:nowrap;" />
                            <spinner id="whdLogo1YPosBox" constraint="" value="1" cols="4" hflex="1" />
                          </hbox>
                        </cell>
                      </hbox>
                  </div>
                  <separator spacing="3px" />
                  <hbox align="center" width="100%" spacing="3px">
                    <checkbox id="whdLogo2Checkbox" />
                    <whlabel1 value="Logo 2:" />
                    <textbutton id="whdLogo2ReplaceBtn" label="Upload" upload="true" />
                    <textbutton id="whdLogo2DownloadBtn" label="Download" />
                    <textbutton id="whdLogo2DeleteBtn" label="Delete" />
                  </hbox>
                  <div id="whdLogo2Area" width="100%">
                      <hbox align="center" width="100%" spacing="3px">
                        <space spacing="16px" />
                        <image id="whdLogo2Image" style="max-height:80px; max-width:200px;" />
                      </hbox>
                      <separator spacing="3px" />
                      <hbox align="center" width="100%" spacing="3px">
                        <space spacing="16px" />
                        <whlabel2 value="Position:" hflex="min" />
                        <listbox id="whdLogo2PositionBox" mold="select" rows="1" >
                          <listitem label="Top-right" value="right"/>
                          <listitem label="Top-left (absolute)" value="absolute-top-left"/>
                          <listitem label="Top-left (fixed)" value="fixed-top-left"/>
                          <listitem label="Top-right (absolute)" value="absolute-top-right"/>
                          <listitem label="Top-right (fixed)" value="fixed-top-right"/>
                          <listitem label="Bottom-left (absolute)" value="absolute-bottom-left"/>
                          <listitem label="Bottom-left (fixed)" value="fixed-bottom-left"/>
                          <listitem label="Bottom-right (absolute)" value="absolute-bottom-right"/>
                          <listitem label="Bottom-right (fixed)" value="fixed-bottom-right"/>
                        </listbox>
                      </hbox>
                      <separator spacing="3px" />
                      <hbox align="center" pack="stretch" width="100%" spacing="4px">
                        <space spacing="16px" />
                        <cell hflex="1">
                          <hbox align="center" pack="stretch">
                            <whlabel2 value="Width&#160;[px]:" />
                            <spinner id="whdLogo2WidthBox" constraint="no negative" value="1" cols="4" hflex="1" />
                          </hbox>
                        </cell>
                        <cell hflex="1">
                          <hbox align="center" pack="stretch">
                            <whlabel2 value="Height&#160;[px]:" />
                            <spinner id="whdLogo2HeightBox" constraint="no negative" value="1" cols="4" hflex="1" />
                          </hbox>
                        </cell>
                      </hbox>
                      <separator spacing="3px" />
                      <hbox align="center" pack="stretch" width="100%" spacing="4px">
                        <space spacing="16px" />
                        <cell hflex="1">
                          <hbox align="center" pack="stretch">
                            <whlabel2 value="X-Pos&#160;[px]:" style="white-space:nowrap;" />
                            <spinner id="whdLogo2XPosBox" constraint="" value="1" cols="4" hflex="1" />
                          </hbox>
                        </cell>
                        <cell hflex="1">
                          <hbox align="center" pack="stretch">
                            <whlabel2 value="Y-Pos&#160;[px]:" style="white-space:nowrap;" />
                            <spinner id="whdLogo2YPosBox" constraint="" value="1" cols="4" hflex="1" />
                          </hbox>
                        </cell>
                      </hbox>
                  </div>
                </tabpanel>
                <!-- Tree Navigation Panel -->
                <tabpanel sclass="control_panel">
                  <vbox align="stretch" width="100%" spacing="2px">
                    <hbox align="center" width="100%" spacing="3px">
                      <whlabel1 value="Panel Width:" hflex="1" />
                      <spinner id="whdNavigationWidthBox" constraint="no negative" cols="4" hflex="min" />
                      <label value="px" hflex="min" />
                    </hbox>
                    <whlabel1 value="Margin:" />
                    <hbox pack="stretch" align="center" width="100%" spacing="0px">
                      <cell hflex="1" align="right">
                        <whlabel2 value="Top&#160;[px]:" />
                        <spinner id="whdNavigationMarginTopBox" constraint="no negative" width="60px" />
                      </cell>
                      <cell width="50%" align="right">
                        <whlabel2 value="Bottom&#160;[px]:" />
                        <spinner id="whdNavigationMarginBottomBox" constraint="no negative" width="60px" />
                      </cell>
                    </hbox>
                    <whlabel1 value="Padding:" />
                    <hbox pack="stretch" align="center" width="100%" spacing="0px">
                      <cell hflex="1" align="right">
                        <whlabel2 value="Top&#160;[px]:" />
                        <spinner id="whdNavigationPaddingTopBox" constraint="no negative" width="60px" />
                      </cell>
                      <cell width="50%" align="right">
                        <whlabel2 value="Bottom&#160;[px]:" />
                        <spinner id="whdNavigationPaddingBottomBox" constraint="no negative" width="60px" />
                      </cell>
                    </hbox>
                    <hbox pack="stretch" align="center" width="100%" spacing="0px">
                      <cell hflex="1" align="right">
                        <whlabel2 value="Left&#160;[px]:" />
                        <spinner id="whdNavigationPaddingLeftBox" constraint="no negative" width="60px" />
                      </cell>
                      <cell width="50%" align="right">
                        <whlabel2 value="Right&#160;[px]:" />
                        <spinner id="whdNavigationPaddingRightBox" constraint="no negative" width="60px" />
                      </cell>
                    </hbox>
                    <hbox align="center" width="100%" spacing="3px">
                      <whlabel1 value="Panel Background:" hflex="1" />
                      <div sclass="box_preview"><div id="whdNavigationBgPrev" sclass="box_prev_inner"></div></div>
                      <button id="whdNavigationBgBtn" label="..." />
                    </hbox>
                    <hbox align="center" width="100%" spacing="3px">
                      <whlabel1 value="Tree Background:" hflex="1" />
                      <div sclass="box_preview"><div id="whdNavigationTreeBgPrev" sclass="box_prev_inner"></div></div>
                      <button id="whdNavigationTreeBgBtn" label="..." />
                    </hbox>
                    <separator spacing="2px" />
                    <hbox align="center" pack="stretch" width="100%" spacing="0px">
                      <vbox hflex="1" spacing="0px">
                        <hbox align="center" pack="stretch" width="100%" spacing="2px">
                          <whlabel1 value="Toggle Button:" hflex="1" />
                          <textbutton id="whdNavigationToggleReplaceBtn" label="Upload" upload="true" />
                          <textbutton id="whdNavigationToggleDownloadBtn" label="Download" />
                          <textbutton id="whdNavigationToggleDeleteBtn" label="Delete" />
                        </hbox>
                        <hbox align="center" pack="stretch" hflex="1" >
                            <space hflex="1" />
                            <whlabel2 value="Width&#160;[px]:" />
                            <spinner id="whdNavigationToggleWidthBox" constraint="no negative" width="60px" />
                            <space spacing="2px" />
                        </hbox>
                      </vbox>
                      <cell width="34px" align="right">
                        <image id="whdNavigationToggleImage" style="max-height:40px; max-width:30px; border:dashed 1px #D0D0D0;" />
                      </cell>
                    </hbox>
                    <separator spacing="2px" />
                    <hbox align="center" width="100%" spacing="3px">
                      <checkbox id="whdNavResizeCheckbox" />
                      <whlabel1 value="Resizable"  />
                    </hbox>
                    <separator spacing="2px" />
                    <hbox align="center" width="100%" spacing="3px">
                      <checkbox id="whdNavAccordionCheckbox" />
                      <whlabel1 value="Accordion Tabs"  hflex="1"/>
                      <whlabel2 value="&#160;Height&#160;[px]:"/>
                      <spinner id="whdNavAccordionHeightBox" constraint="no negative" width="46px" />
                    </hbox>
                    <hbox align="center" width="100%" spacing="3px">
                      <space spacing="16px" />
                      <button id="whdNavAccordionBgBtn" label="Background..." style="white-space:nowrap;" />
                      <button id="whdNavAccordionFontBtn" label="Font..." style="white-space:nowrap;" />
                      <space hflex="1" />
                      <div id="whdNavAccordionBgPrev" width="50px" style="background-color:#FFFFFF;"><label id="whdNavAccordionFontPrev" value="Abc" /></div>
                    </hbox>
                    <hbox align="center" pack="end" width="100%" spacing="3px">
                      <whlabel2 value="Indent&#160;[px]:" />
                      <spinner id="whdNavAccordionPaddingXBox" constraint="no negative" width="46px" />
                      <whlabel2 value="&#160;Spacing&#160;[px]:"/>
                      <spinner id="whdNavAccordionSpacingBox" constraint="no negative" width="46px" />
                    </hbox>
                  </vbox>
                </tabpanel>
                <!-- Tree Style Panel -->
                <tabpanel sclass="control_panel">
                  <vbox align="stretch" width="100%" spacing="2px">
                    <!--
                    <hbox align="center" width="100%" spacing="3px">
                      <whlabel1 value="Tree-Style:" hflex="1" />
                      <listbox id="whdNavTreeStyleBox" mold="select" rows="1" >
                        <listitem label="dotted" value="dotted"/>
                        <listitem label="triangle" value="triangle"/>
                      </listbox>
                    </hbox>
                    -->
                    <hbox align="center" width="100%" spacing="3px">
                      <whlabel1 value="Tree-Animation:" hflex="1" />
                      <listbox id="whdNavTreeAnimationBox" mold="select" rows="1" >
                        <listitem label="off" value="off"/>
                        <listitem label="slow" value="slow"/>
                        <listitem label="medium" value="medium"/>
                        <listitem label="fast" value="fast"/>
                      </listbox>
                    </hbox>
                    <hbox align="center" width="100%" spacing="0px">
                      <checkbox id="whdNavTreeCollapsedBox" />
                      <whlabel1 value="Collapsed" />
                      <space spacing="4px" />
                      <checkbox id="whdNavTreeAutoCloseBox" />
                      <whlabel1 value="Auto-Close" />
                    </hbox>
                    <hbox align="center" width="100%" spacing="0px">
                      <checkbox id="whdNavTreeShowTitleBox" />
                      <whlabel1 value="Show Tree Title" hflex="1" />
                      <cell valign="middle" sclass="font_preview"><label id="whdNavigationFontTitlePrev" sclass="font_prev_inner" value="Abc" /></cell>
                      <space spacing="3px" />
                      <button id="whdNavigationFontTitleBtn" label="..." />
                    </hbox>
                    <hbox align="center" width="100%" spacing="3px">
                      <whlabel1 value="Font (default):" hflex="1" />
                      <cell valign="middle" sclass="font_preview"><label id="whdNavigationFontPrev" sclass="font_prev_inner" value="Abc" /></cell>
                      <button id="whdNavigationFontBtn" label="..." />
                    </hbox>
                    <hbox align="center" width="100%" spacing="3px">
                      <whlabel1 value="Font (active):" hflex="1" />
                      <cell valign="middle" sclass="font_preview"><label id="whdNavigationFontCurrentPrev" sclass="font_prev_inner" value="Abc" /></cell>
                      <button id="whdNavigationFontCurrentBtn" label="..." />
                    </hbox>
                    <hbox align="center" width="100%" spacing="3px">
                      <whlabel1 value="Font (mouse-over):" hflex="1" />
                      <cell valign="middle" sclass="font_preview"><label id="whdNavigationFontHoverPrev" sclass="font_prev_inner" value="Abc" /></cell>
                      <button id="whdNavigationFontHoverBtn" label="..." />
                    </hbox>
                    <separator spacing="2px" />
                    <hbox align="center" pack="stretch" width="100%" spacing="0px">
                      <vbox hflex="1">
                        <hbox align="center" width="100%" spacing="3px">
                          <whlabel1 value="Content Icon:" hflex="1" />
                          <textbutton id="whdNavTreeContentImgReplaceBtn" label="Upload" upload="true" />
                          <textbutton id="whdNavTreeContentImgDownloadBtn" label="Download" />
                          <textbutton id="whdNavTreeContentImgDeleteBtn" label="Delete" />
                        </hbox>
                        <hbox align="center" pack="stretch" hflex="1">
                            <space hflex="1" />
                            <whlabel2 value="Width&#160;[px]:" />
                            <spinner id="whdNavTreeContentImgWidthBox" constraint="no negative" width="60px" />
                            <space spacing="2px" />
                        </hbox>
