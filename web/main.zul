                                       onSelect="SettingsResetBtn.disabled = false"></listbox>
                            </hbox>
                            <separator spacing="5px"/>
                            <hbox pack="start" align="center">
                              <label style="font-weight:bold;">Maximum number of Revisions per User:</label>
                              <intbox id="SettingsMaxRevisionsIntbox" constraint="no negative" maxlength="2" cols="3" 
                                      onChanging="SettingsResetBtn.disabled = false" />
                            </hbox>
                            <separator spacing="5px"/>
                            <hbox pack="start" align="center">
                              <label style="font-weight:bold;">Maximum number of Nodes in HTML-Preview:</label>
                              <intbox id="SettingsPreviewMaxHTMLIntbox" constraint="no negative" maxlength="9" cols="5" 
                                      onChanging="SettingsResetBtn.disabled = false" />
                              <label style="font-weight:normal;">(0 = unlimited)</label>
                            </hbox>
                            <separator spacing="5px"/>
                            <hbox pack="start" align="center">
                              <label style="font-weight:bold;">Maximum number of Nodes in Print-Preview:</label>
                              <intbox id="SettingsPreviewMaxPrintIntbox" constraint="no negative" maxlength="9" cols="5" 
                                      onChanging="SettingsResetBtn.disabled = false" />
                              <label style="font-weight:normal;">(0 = unlimited)</label>
                            </hbox>
                            <separator spacing="5px"/>
                            <label style="font-weight:bold;">Text File Extensions:</label>
                            <textbox id="SettingsTxtFileExtensionsTextbox" width="98%" 
                                     onChanging="SettingsResetBtn.disabled = false" />
                            <separator spacing="5px"/>
                            <hbox pack="start" align="center" spacing="3px">
                                <label style="font-weight:bold;">Custom FOP Configuration File</label>
                                <label>&#160;(see</label>
                                <toolbarbutton label="Help" style="color:#0000FF" onClick='mainWin.onHelp("help/app_settings.html")'/>
                                <label>on how to create language dependent configuration):</label>
                            </hbox>
                            <textbox id="SettingsFOPConfigPathTextbox" width="98%" 
                                     onChanging="SettingsResetBtn.disabled = false" />
                            <separator spacing="3px"/>
                          </groupbox>
                          <space spacing="2px" />
                          <vbox style="min-width:100px;" hflex="min" spacing="2px">
                            <button id="SettingsSaveBtn" label="${c:l('label.save.btn')}" 
                                    image="img/save.gif" hflex="1" forward="mainWin.onSaveApplicationSettings" />
                            <button id="SettingsResetBtn" label="${c:l('label.reset.btn')}" disabled="true"
                                    hflex="1" forward="mainWin.onResetAppSettingsPanel" />
                          </vbox>
                        </hbox>
                      </vlayout>
                    </tabpanel>
                </tabpanels>
                </tabbox>
<!-- End: Admin Tabpanel. -->
            </tabpanel>
        </tabpanels>
    </tabbox>
      
    </center>
    <south height="4px" style="background-color:transparent;"  border="none">
    </south>
    </borderlayout>
    <!-- </vbox> -->
    </div>
    <script type="text/javascript"><![CDATA[
        function sendMainWindowEvent(evtname, data) {
            zAu.send(new zk.Event(zk.Widget.$('$mainWin'), evtname, data));
        }

        function openEditWin(node_id) {
            zAu.send(new zk.Event(zk.Widget.$('$mainWin'), 'onEditContentClick', node_id));
        }

        function previewNodeByAlias(node_alias) {
            zAu.send(new zk.Event(zk.Widget.$('$mainWin'), 'onPreviewContentByAlias', node_alias));
        }

        function previewNodeById(node_id) {
            zAu.send(new zk.Event(zk.Widget.$('$mainWin'), 'onPreviewNodeById', node_id));
        }

        function processDeferredDocmaEvents() {
            zAu.send(new zk.Event(zk.Widget.$('$mainWin'), 'onProcessDeferredEvents', null));
        }

        function editNodePropsById(node_id) {
            zAu.send(new zk.Event(zk.Widget.$('$mainWin'), 'onEditNodePropsById', node_id));
        }

        function setSelectedNodes(id_list) {
            zAu.send(new zk.Event(zk.Widget.$('$mainWin'), 'onSetSelectedNodesByIds', id_list));
        }

        function addSelectedNodes(id_list) {
            zAu.send(new zk.Event(zk.Widget.$('$mainWin'), 'onAddSelectedNodesByIds', id_list));
        }

        function downloadFiles() {
            zAu.send(new zk.Event(zk.Widget.$('$mainWin'), 'onDownloadFile', null));
        }

        function uploadFiles() {
            zAu.send(new zk.Event(zk.Widget.$('$mainWin'), 'onUploadFile', null));
        }

        function deleteFiles() {
            zAu.send(new zk.Event(zk.Widget.$('$mainWin'), 'onDeleteFileNodes', null));
        }

        function previewGoBackward() {
            var frm_win = window.frames['viewcontentfrm'];
            var pname = frm_win.location.pathname;
            if ((frm_win.history.length > 0) && (pname.length > 0) && (pname.indexOf('empty_preview.html') < 0)) {
                window.frames['viewcontentfrm'].history.back();
            }
        }

        function previewGoForward() {
            window.frames['viewcontentfrm'].history.forward();
        }

        function previewRefresh() {
            window.frames['viewcontentfrm'].location.reload();
        }

        function previewContentRefresh() {
            var p_url = window.frames['viewcontentfrm'].location.href;
            if ((p_url.indexOf('viewContent.jsp') >= 0) || (p_url.indexOf('viewPDF.jsp') >= 0)) {
                window.frames['viewcontentfrm'].location.reload();
            }
        }

        var docma_client_props = new Object();

        function getDocmaClientProp(propName) {
            return docma_client_props[propName];
        }

        function setDocmaClientProp(propName, propValue) {
            docma_client_props[propName] = propValue;
        }
    ]]></script>
</window>

