<?xml version="1.0" encoding="UTF-8"?>
<?page title="Docmenta" ?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>

<?component name="headlabel" extends="label" style="color:#DFEEFF; text-shadow:1px 1px 1px #000000;" ?>
<?component name="headlabelbold" extends="label" style="color:#DFEEFF; font-weight:bold; text-shadow:1px 1px 1px #000000;" ?>
<?component name="headlabelsmall" extends="label" style="color:#DFEEFF; text-shadow:1px 1px 1px #404040;" ?>
<?component name="headbutton" extends="toolbarbutton" sclass="headBtnCls" ?>
<?component name="textbutton" extends="toolbarbutton" style="font-size:11px" ?>
<?component name="explorerwin" extends="window" mode="overlapped" border="normal" sizable="true" draggable="false"
            maximizable="true" minimizable="false" minwidth="100" minheight="100" ?> 

<zk>
<style>
table { border-collapse:collapse; }
.headBtnCls .z-toolbarbutton-cnt { color:#DFEEFF;font-weight:bold;font-size:11px;text-shadow:1px 1px 1px #202020; }
.z-toolbarbutton-over .z-toolbarbutton-cnt { color:#202020;text-shadow:0 0 0; }
div.z-treecol-cnt { height:16px; max-height:16px; margin:0; padding:0; color:#000000; font-weight:normal; }
div.z-listheader-cnt { height:16px; max-height:16px; color:#000000; font-weight:normal !important; overflow:hidden; }
.z-menubar-hor { position : relative; display: block; padding : 2px 0px; border:0px; background-image:url(img/tab_toolbar_bg2.png);background-repeat:repeat-x;background-position:0px -1px; }
.z-tabs-ver { padding-top:1px; padding-left:1px; background-color:#AFD6E8; background-image:url(img/tab_vertical_bg.png);background-repeat:repeat-y; }
.z-popup .z-popup-cnt { color:#202020; }
.z-listbox-autopaging .z-listcell-cnt { height: 22px; overflow: hidden; }
.doctreecell img { height:16px; }
.doctoolbtn img { height:17px; }
.docitemseltype img { height:32px; }
</style>
<window id="mainWin"  height="100%" width="100%" border="none" mode="embedded"
        contentStyle="background-color:#070604"
        apply="org.docma.webapp.Main" use="org.docma.webapp.MainWindow">

    <div id="MainWinContentArea" style="padding:0 10px 0 10px; height:100%; background-image:url(img/docmenta_back.jpg); background-repeat:no-repeat; background-position:0px 0px;">
    <!-- <vbox width="100%" height="100%" heights="60px,*"> -->
    <borderlayout style="background-color:transparent;">
    <north height="60px" margins="10,10,10,0" style="background-color:transparent;" border="none">
      <hbox width="100%" widths=",,99%">
        <image id="MainWinAppLogo" src="img/docmenta_logo.png" height="52px" width="218px" />
        <space spacing="8px" />
        <borderlayout height="60px" style="background-color:transparent;">
        <west style="background-color:transparent;" border="none" margins="0,0,0,0">
          <vbox>
            <separator spacing="28px"/>
            <headlabelsmall id="appversion"></headlabelsmall>
            <timer id="MainWinProcessEventsTimer"
                   delay="180000" repeats="true" running="true"
                   forward="onTimer=mainWin.onProcessDeferredEvents" />
          </vbox>
        </west>
        <east style="background-color:transparent;" border="none" size="80%">
          <vbox spacing="8px" width="100%" align="end">
            <hbox align="center">
              <headbutton label="${c:l('label.about')}" image="img/about_16.png" forward="mainWin.onAbout" />
              <space spacing="5px" />
              <headbutton label="${c:l('label.help')}" image="img/help_16.png" onClick='mainWin.onHelp("help/index.html")' />
              <space spacing="5px" />
              <headbutton id="EditProfileHeadButton" label="${c:l('label.edituserprofile')}"
                          image="img/user_16.png" forward="mainWin.onEditUserProfile" />
              <space spacing="5px" />
              <headbutton label="${c:l('label.logout.btn')}" image="img/on_off_16.gif" forward="mainWin.onLogout" />
            </hbox>
            <hbox align="center">
    	      <headlabelbold>${c:l('label.product')}:</headlabelbold>
              <listbox id="storesList" mold="select" rows="1" onSelect="mainWin.onSelectStore()"></listbox>
              <space spacing="2px" />
              <headlabelbold>${c:l('label.productversion')}:</headlabelbold>
              <listbox id="versionsList" mold="select" rows="1" onSelect="mainWin.onSelectVersion()"></listbox>
              <space spacing="2px" />
              <headlabelbold>${c:l('label.language')}:</headlabelbold>
              <listbox id="languageList" mold="select" rows="1" onSelect="mainWin.onSelectContentLanguage()"></listbox>
            </hbox>
            <!--
            <borderlayout height="30px" style="background-color:transparent;">
            <east style="background-color:transparent;" border="none">
            </east>
            </borderlayout>
            -->
          </vbox>		  
        </east>
        </borderlayout>
      </hbox>
    </north>
    <center style="background-color:transparent;" border="none">

    <tabbox id="mainTabbox" width="100%" height="100%" tabscroll="true">  <!-- tabscroll="false" -->
        <tabs style="background-color:#E0F3FC;padding-top:2px;padding-left:4px;background-image:url(img/tab_toolbar_bg2.png);background-repeat:repeat-x;" >
            <tab id="contentTab" label="${c:l('tab.content')}" forward="onSelect=mainWin.onSelectContentTab"/>
            <tab id="stylesTab" label="${c:l('tab.styles')}" forward="onSelect=mainWin.onSelectStylesTab"/>
            <tab id="versioningTab" label="${c:l('tab.versioning')}" forward="onSelect=mainWin.onSelectVersioningTab"/>
            <tab id="publishingTab" label="${c:l('tab.publishing')}" forward="onSelect=mainWin.onSelectPublishingTab"/>
            <tab id="adminTab" label="${c:l('tab.admin')}" forward="onSelect=mainWin.onSelectAdminTab"/>
        </tabs>
        <toolbar id="mainTabToolbar" xmlns:w="http://www.zkoss.org/2005/zk/client" style="padding-top:0;">
        <hbox id="docTreeToolbarBox" align="center">
          <toolbarbutton id="previewPDFBtn" image="img/pdf_icon.gif" onClick="mainWin.onPreviewPDF()" sclass="doctoolbtn" />
          <space spacing="2px" />
          <toolbarbutton id="previewBackwardBtn" image="img/arrow_left.gif" w:onClick="previewGoBackward();" />
          <toolbarbutton id="previewForwardBtn" image="img/arrow_right.gif" w:onClick="previewGoForward();" />
          <space spacing="2px" />
          <toolbarbutton id="previewRefreshBtn" image="img/refresh_icon.gif" w:onClick="previewRefresh();" />
          <space spacing="2px" />
          <label style="font-weight:bold;">${c:l('label.contenttoolbar.preview')}</label>
          <listbox id="previewFormatList" mold="select" rows="1" onSelect="mainWin.onSelectPreviewFormat()">
            <listitem label="HTML" value="html"/>
            <listitem label="PDF" value="pdf"/>
          </listbox>
          <listbox id="previewPubConfList" mold="select" rows="1" onSelect="mainWin.onSelectPreviewPub()">
            <listitem label="unfiltered" value=""/>
          </listbox>
          <listbox id="previewOutputConfList" mold="select" rows="1" onSelect="mainWin.onSelectPreviewOutput()">
            <listitem label="default" value=""/>
          </listbox>
          <!--
          <space spacing="3px" />
          <button id="switchContentLayoutBtn" image="img/hmode.gif" onClick="mainWin.onSwitchContentLayout()" />
          -->
          <space spacing="2px" />
          <menubar id="docTreeMenubar" style="background-color:transparent;">
            <menu label="Menu" style="background-color:transparent;">
              <menupopup>
                <menuitem id="mainMenuitemShowIndexTerms" label="${c:l('label.menuitem.showindexterms')}" checkmark="true" autocheck="true" forward="mainWin.onTogglePreviewIndexTerms" />
                <menuitem id="mainMenuitemFindByAlias" label="${c:l('label.menuitem.findbyalias')}" forward="mainWin.onFindByAliasAll" />
                <menuitem id="mainMenuitemClearCutList" label="${c:l('label.menuitem.clearcutlist')}" forward="mainWin.onClearCutCopyList" />
                <menuseparator/>
                <menuitem id="mainMenuitemSwitchLayout" label="${c:l('label.menuitem.switchlayout')}" forward="mainWin.onSwitchContentLayout" />
                <menuseparator/>
                <menuitem id="mainMenuitemShowPreviewLog" label="${c:l('label.menuitem.showpreviewlog')}" forward="mainWin.onShowPreviewLog" />
              </menupopup>
            </menu>
          </menubar>
        </hbox>
        </toolbar>
        <tabpanels style="background-color:#FFFFFF;">
            <tabpanel id="contentTabpanel" style="margin:0px;padding:0px;" >
<!-- Start: Content Tabpanel. -->
<menupopup id="treemenu" forward="onOpen=mainWin.onOpenTreeMenu"></menupopup>
<menupopup id="no_treemenu"></menupopup>
<!-- <vbox width="100%" height="100%" spacing="0" > -->
<borderlayout id="contentBorderLayout" width="100%" height="100%">
<center border="normal">
<!-- <splitter collapse="before" /> -->
    <iframe id="viewcontentfrm" name="viewcontentfrm" src="empty_preview.html" width="100%" height="100%"/>
<!-- </vbox> -->
</center>
</borderlayout>
<!-- End: Content Tabpanel. -->
            </tabpanel>
            <tabpanel id="StylesTabpanel" height="100%" style="overflow:scroll;padding-right:0;">
<!-- Start: Styles Tabpanel. -->
<vlayout width="100%" >
  <hbox align="center">
    <label value="Type:" style="font-weight:bold;"/>
    <listbox id="StylesTypeFilterList" mold="select" rows="1" forward="onSelect=mainWin.onChangeStylesFilter">
      <listitem label="${c:l('label.styles.filter.inline')}" value="inline"/>
      <listitem label="${c:l('label.styles.filter.block')}" value="block"/>
    </listbox>
    <space spacing="4px" />
    <hbox id="StylesVariantFilterArea" align="center" visible="false">
      <label value="Variant:" style="font-weight:bold;"/>
      <listbox id="StylesVariantFilterList" mold="select" rows="1" forward="onSelect=mainWin.onChangeStylesFilter"></listbox>
    </hbox>
  </hbox>
  <hbox width="100%" align="start" pack="stretch">
    <cell hflex="1">
      <listbox id="StylesListbox" width="100%" sizedByContent="false" multiple="true" context="StylesContextmenu">
        <listhead sizable="true">
            <listheader label="${c:l('label.style.id')}" width="20%" />
            <listheader label="${c:l('label.style.variant')}" width="15%" />
            <listheader label="${c:l('label.style.name')}" width="30%" />
            <listheader label="${c:l('label.style.hidden')}" width="5%" />
            <listheader label="${c:l('label.style.preview')}" width="30%" />
        </listhead>
      </listbox>
    </cell>
    <space spacing="2px" />
    <cell hflex="min">
      <vbox align="stretch" hflex="min" spacing="2px" style="min-width:126px;">
        <button id="NewStyleBtn" label="${c:l('label.styles.new.btn')}" hflex="1" image="img/add.gif" forward="mainWin.onNewStyle"/>
        <button id="EditStyleBtn" label="${c:l('label.styles.edit.btn')}" hflex="1" image="img/edit_style.gif" forward="mainWin.onEditStyle" />
        <button id="DeleteStyleBtn" label="${c:l('label.styles.delete.btn')}" hflex="1" image="img/delete.gif" forward="mainWin.onDeleteStyle" />
        <button id="VariantStyleBtn" label="${c:l('label.styles.createvariant.btn')}" hflex="1" image="img/create_variant.gif" forward="mainWin.onVariantStyle" />
        <separator spacing="8px" />
        <button id="ImportStyleBtn" label="${c:l('label.styles.import.btn')}" hflex="1" forward="mainWin.onImportStyle" />
        <button id="ExportStyleBtn" label="${c:l('label.styles.export.btn')}" hflex="1" forward="mainWin.onExportStyle" />

        <menupopup id="StylesContextmenu">
          <menuitem id="NewStyleMenuitem" label="${c:l('label.styles.new.btn')}" image="img/add.gif" forward="mainWin.onNewStyle"/>
          <menuitem id="EditStyleMenuitem" label="${c:l('label.styles.edit.btn')}" image="img/edit_style.gif" forward="mainWin.onEditStyle"/>
          <menuitem id="DeleteStyleMenuitem" label="${c:l('label.styles.delete.btn')}" image="img/delete.gif" forward="mainWin.onDeleteStyle"/>
          <menuitem id="VariantStyleMenuitem" label="${c:l('label.styles.createvariant.btn')}" image="img/create_variant.gif" forward="mainWin.onVariantStyle"/>
          <menuseparator/>
          <menuitem id="ImportStyleMenuitem" label="${c:l('label.styles.import.btn')}" forward="mainWin.onImportStyle"/>
          <menuitem id="ExportStyleMenuitem" label="${c:l('label.styles.export.btn')}" forward="mainWin.onExportStyle"/>
        </menupopup>
      </vbox>
    </cell>
    <space spacing="2px" />
  </hbox>
</vlayout>
<!-- End: Styles Tabpanel. -->
            </tabpanel>
            <tabpanel id="VersioningTabpanel" width="100%" height="100%">
<!-- Start: Versioning Tabpanel. -->
             <!-- <hlayout width="98%" height="100%" style="overflow:scroll;"> -->
             <borderlayout width="100%" height="100%" style="background-color:transparent;">
               <center border="none" flex="true" margins="0,0,5,8" style="background-color:#FFFFFF;" >
                  <listbox id="VersionsListbox" width="100%" sizedByContent="false" 
                           multiple="true" hflex="1" context="VersionsContextmenu" >
                    <listhead sizable="true">
                      <listheader label="${c:l('label.productversion.list.id')}" sort="auto(versionId)" width="20%"/>
                      <listheader label="${c:l('label.productversion.list.language')}" sort="auto(languageCode)" width="5%" />
                      <listheader label="${c:l('label.productversion.list.state')}" sort="auto(state)" width="10%" />
                      <listheader label="${c:l('label.productversion.list.createdate')}" sort="auto(creationDate)" width="15%" />
                      <!-- <listheader label="Last Modified" sort="auto(lastModified)" /> -->
                      <listheader label="${c:l('label.productversion.list.releasedate')}" sort="auto(releaseDate)" width="15%" />
                      <listheader label="${c:l('label.productversion.list.derivedfrom')}" sort="auto(derivedFrom)" width="20%" />
                      <listheader label="${c:l('label.productversion.list.comment')}" sort="auto(comment)" width="15%" />
                    </listhead>
                  </listbox>
                </center>
                <east border="none" size="164px" margins="0,4,8,0">
                  <vlayout width="164px" spacing="2px">
                    <button id="CreateVersionBtn" label="${c:l('label.productversion.create.btn')}" width="160px" image="img/add.gif" forward="mainWin.onCreateVersion" />
                    <button id="DeleteVersionBtn" label="${c:l('label.productversion.delete.btn')}" width="160px" image="img/delete.gif" forward="mainWin.onDeleteVersion" />
                    <separator spacing="18px"/>
                    <button id="ReleaseVersionBtn" label="${c:l('label.productversion.release.btn')}" width="160px" forward="mainWin.onReleaseVersion" />
                    <button id="UnreleaseVersionBtn" label="${c:l('label.productversion.unrelease.btn')}" width="160px" forward="mainWin.onUnreleaseVersion" />
                    <button id="RenameVersionBtn" label="${c:l('label.productversion.rename.btn')}" width="160px" forward="mainWin.onRenameVersion" />
                    <button id="EditVersionCommentBtn" label="${c:l('label.productversion.editcomment.btn')}" width="160px" forward="mainWin.onEditVersionComment" />
                    <separator spacing="18px"/>
                    <button id="ClearRevisionsBtn" label="${c:l('label.productversion.clearrevisions.btn')}" width="160px" forward="mainWin.onClearRevisions" />
                    <separator spacing="18px"/>
                    <button id="VerShowConnectedBtn" label="${c:l('label.connectedusers.show.btn')}" width="160px" forward="mainWin.onShowConnectedUsers" />
                    <menupopup id="VersionsContextmenu">
                      <menuitem id="CreateVersionMenuitem" label="${c:l('label.productversion.create.btn')}" image="img/add.gif" forward="mainWin.onCreateVersion" />
                      <menuitem id="DeleteVersionMenuitem" label="${c:l('label.productversion.delete.btn')}" image="img/delete.gif" forward="mainWin.onDeleteVersion" />
                      <menuseparator/>
                      <menuitem id="ReleaseVersionMenuitem" label="${c:l('label.productversion.release.btn')}" forward="mainWin.onReleaseVersion" />
                      <menuitem id="UnreleaseVersionMenuitem" label="${c:l('label.productversion.unrelease.btn')}" forward="mainWin.onUnreleaseVersion" />
                      <menuitem id="RenameVersionMenuitem" label="${c:l('label.productversion.rename.btn')}" forward="mainWin.onRenameVersion" />
                      <menuitem id="EditVersionCommentMenuitem" label="${c:l('label.productversion.editcomment.btn')}" forward="mainWin.onEditVersionComment" />
                      <menuseparator/>
                      <menuitem id="ClearRevisionsMenuitem" label="${c:l('label.productversion.clearrevisions.btn')}" forward="mainWin.onClearRevisions" />
                    </menupopup>
                  </vlayout>
                </east>
              </borderlayout>
<!-- End: Versioning Tabpanel. -->
            </tabpanel>
            <tabpanel id="PublishingTabpanel" style="padding:0;margin:0;">
<!-- Start: Publishing Tabpanel. -->
                <tabbox id="PublishingTabbox" orient="vertical" height="100%" width="100%" tabscroll="true">
                <tabs width="150px">
                    <tab label="Configuration"/>
                    <tab label="Applicabilities"/>
                    <tab label="Export"/>
                    <!-- <tab label="Filter settings"/> -->
                </tabs>
                <tabpanels style="background-color:#FFFFFF;">
                    <tabpanel id="PublishingConfigTabpanel" width="100%" height="100%" style="padding-right:0;">
                        <vlayout width="100%" height="100%" style="overflow:scroll;">
                            <label value="Publications:" style="font-weight:bold;"/>
                            <hbox width="100%" align="start" pack="stretch">
                              <listbox id="PublicationConfigListbox" width="100%" sizedByContent="false" context="PublicationConfigContextmenu">
                                <listhead sizable="true">
                                  <listheader label="${c:l('label.pubconfig.list.id')}" sort="auto(id)" width="20%" />
                                  <listheader label="${c:l('label.pubconfig.list.title')}" sort="auto(title)" width="35%" />
                                  <listheader label="${c:l('label.pubconfig.list.contentroot')}" width="20%" />
                                  <listheader label="${c:l('label.pubconfig.list.filter')}" width="25%" />
                                </listhead>
                              </listbox>
                              <space spacing="2px" />
                              <vbox width="80px" spacing="2px" align="center">
                                <button id="NewPublicationConfigBtn" label="${c:l('label.new.btn')}" width="80px" forward="mainWin.onNewPublicationConfig"/>
                                <button id="EditPublicationConfigBtn" label="${c:l('label.edit.btn')}" width="80px" forward="mainWin.onEditPublicationConfig"/>
                                <button id="DeletePublicationConfigBtn" label="${c:l('label.delete.btn')}" width="80px" forward="mainWin.onDeletePublicationConfig"/>
                                <button id="CopyPublicationConfigBtn" label="${c:l('label.copy.btn')}" width="80px" forward="mainWin.onCopyPublicationConfig"/>
                                <menupopup id="PublicationConfigContextmenu">
                                  <!-- <menuitem id="NewPublicationConfigMenuitem" label="${c:l('label.new.btn')}" forward="mainWin.onNewPublicationConfig"/> -->
                                  <menuitem id="EditPublicationConfigMenuitem" label="${c:l('label.edit.btn')}" forward="mainWin.onEditPublicationConfig"/>
                                  <menuitem id="DeletePublicationConfigMenuitem" label="${c:l('label.delete.btn')}" forward="mainWin.onDeletePublicationConfig"/>
                                  <menuitem id="CopyPublicationConfigMenuitem" label="${c:l('label.copy.btn')}" forward="mainWin.onCopyPublicationConfig"/>
                                </menupopup>
                              </vbox>
                              <space spacing="4px" />
                            </hbox>
                            <separator spacing="2px"/>
                            <label value="Output Configurations:" style="font-weight:bold;"/>
                            <hbox width="100%" height="100%" align="start" pack="stretch">
                              <listbox id="MediaConfigListbox" width="100%" sizedByContent="false" context="MediaConfigContextmenu">
                                <listhead sizable="true">
                                  <listheader label="${c:l('label.outconfig.list.id')}" sort="auto(id)" width="35%" />
                                  <listheader label="${c:l('label.outconfig.list.format')}" sort="auto(format)" width="20%" />
                                  <listheader label="${c:l('label.outconfig.list.variant')}" sort="auto(styleVariant)" width="20%" />
                                  <listheader label="${c:l('label.outconfig.list.filter')}" width="25%" />
                                </listhead>
                              </listbox>
                              <space spacing="2px" />
                              <vbox width="80px" spacing="2px" align="center">
                                <button id="NewMediaConfigBtn" label="${c:l('label.new.btn')}" width="80px" forward="mainWin.onNewMediaConfig"/>
                                <button id="EditMediaConfigBtn" label="${c:l('label.edit.btn')}" width="80px" forward="mainWin.onEditMediaConfig"/>
                                <button id="DeleteMediaConfigBtn" label="${c:l('label.delete.btn')}" width="80px" forward="mainWin.onDeleteMediaConfig"/>
                                <button id="CopyMediaConfigBtn" label="${c:l('label.copy.btn')}" width="80px" forward="mainWin.onCopyMediaConfig"/>
                                <menupopup id="MediaConfigContextmenu">
                                  <!-- <menuitem id="NewMediaConfigMenuitem" label="${c:l('label.new.btn')}" forward="mainWin.onNewMediaConfig"/> -->
                                  <menuitem id="EditMediaConfigMenuitem" label="${c:l('label.edit.btn')}" forward="mainWin.onEditMediaConfig"/>
                                  <menuitem id="DeleteMediaConfigMenuitem" label="${c:l('label.delete.btn')}" forward="mainWin.onDeleteMediaConfig"/>
                                  <menuitem id="CopyMediaConfigMenuitem" label="${c:l('label.copy.btn')}" forward="mainWin.onCopyMediaConfig"/>
                                </menupopup>
                              </vbox>
                              <space spacing="4px" />
                            </hbox>
                        </vlayout>
                    </tabpanel>
                    <tabpanel id="ApplicTabpanel" width="100%" height="100%" style="overflow:scroll;padding-right:0;">
                      <hbox width="100%" pack="stretch" vflex="1" hflex="1">
                        <listbox id="DeclaredApplicsListbox" hflex="1" context="DeclaredApplicsContextmenu">
                            <listhead sizable="false">
                              <listheader label="${c:l('label.declaredapplics.list.name')}" />
                            </listhead>
                        </listbox>
                        <space spacing="2px" />
                        <cell hflex="min">
                          <vbox spacing="2px" align="stretch" hflex="min">
                            <button id="AddApplicBtn" label="${c:l('label.add.btn')}" hflex="1" forward="mainWin.onAddApplic"/>
                            <button id="DeleteApplicBtn" label="${c:l('label.delete.btn')}" hflex="1" forward="mainWin.onDeleteApplic" />
                            <menupopup id="DeclaredApplicsContextmenu">
                              <menuitem id="AddApplicMenuitem" label="${c:l('label.add.btn')}" forward="mainWin.onAddApplic"/>
                              <menuitem id="DeleteApplicMenuitem" label="${c:l('label.delete.btn')}" forward="mainWin.onDeleteApplic" />
                            </menupopup>
                          </vbox>
                        </cell>
                        <space spacing="5px" />
                      </hbox>
                    </tabpanel>
                    <tabpanel id="PublishingExportTabpanel" width="100%" height="100%">
                      <hbox width="100%" height="100%" vflex="1" hflex="1" align="start" pack="stretch">
                          <listbox id="PublicationExportListbox" height="100%" multiple="true"
                                   hflex="1" vflex="1" context="PublicationExportContextmenu">
                            <listhead sizable="true">
                              <listheader label="${c:l('label.pubexport.list.pubconf')}" width="12%" />
                              <listheader label="${c:l('label.pubexport.list.outconf')}"  width="12%"/>
                              <listheader label="${c:l('label.pubexport.list.state')}" width="10%"/>
                              <listheader label="${c:l('label.pubexport.list.releasedate')}" width="10%" />
                              <listheader label="${c:l('label.pubexport.list.exportdate')}" width="10%" />
                              <listheader label="${c:l('label.pubexport.list.exportedby')}" width="10%" />
                              <listheader label="${c:l('label.pubexport.list.exportfiles')}" width="26%" />
                              <listheader label="${c:l('label.pubexport.list.online')}" width="10%"/>
                            </listhead>
                          </listbox>
                        <space spacing="2px" />
                        <cell hflex="min">
                          <vbox hflex="min" spacing="2px" align="stretch" style="min-width:100px;" >
                            <button id="NewExportBtn" label="${c:l('label.pubexport.new.btn')}" hflex="1" forward="mainWin.onExportPublication"/>
                            <button id="EditExportBtn" label="${c:l('label.pubexport.edit.btn')}" hflex="1" forward="mainWin.onEditPublication"/>
                            <button id="DeleteExportBtn" label="${c:l('label.pubexport.delete.btn')}" hflex="1" forward="mainWin.onDeletePublication" />
                            <separator spacing="18px"/>
                            <button id="PubShowQueueBtn" label="${c:l('label.pubexport.showqueue.btn')}" hflex="1" forward="mainWin.onShowExportQueue" />
